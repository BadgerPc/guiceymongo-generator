/* Represents a single person */
data Person {
    /* The MongoDB Identity of this object */
    [identity]
    string identity;

	/* Color of eye */
	enum EyeColor {
		Blue,
		Brown,
		Green,
		Hazel
	}
	
	/* Full name */
	string name;
	
	date birth_date;
	
	/* testing */
	EyeColor eye_color;
}